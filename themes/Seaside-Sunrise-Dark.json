  "tokenColors": [
    {
      "name": "Constants & Modifiers",
      "scope": ["constant", "storage.modifier"],
      "settings": { "foreground": "#db605c" }
    },

    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "entity.name.method",
        "keyword.other.get",
        "keyword.other.set",
        "keyword.other.add",
        "keyword.other.remove"
      ],
      "settings": { "foreground": "#bea7eb" }
    },
    {
      "name": "Functions (Delegates)",
      "scope": [
        "keyword.other.delegate",
        "entity.name.type.delegate",
        "support.function"
      ],
      "settings": { "foreground": "#a491cb" }
    },

    {
      "name": "Source",
      "scope": ["source"],
      "settings": { "foreground": "#d6d3de" }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": { "foreground": "#B5B5BA" }
    },
    {
      "name": "Parenthesis & <>",
      "scope": [
        "punctuation.parenthesis",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#9B9BA1" }
    },
    {
      "name": "Square brackets & Curly braces",
      "scope": ["punctuation.squarebracket", "punctuation.curlybrace"],
      "settings": { "foreground": "#818188" }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#67676E", "fontStyle": "italic" }
    },

    {
      "name": "Type declarations and references",
      "scope": [
        "entity.name.type",
        "storage.type",
        "entity.name.type.type-parameter"
      ],
      "settings": { "foreground": "#65b8c2" }
    },
    {
      "name": "Built-in Types",
      "scope": ["keyword.type"],
      "settings": { "foreground": "#b882ee" }
    },
    {
      "name": "Types (Support)",
      "scope": ["support.type"],
      "settings": { "foreground": "#744A9E" }
    },

    {
      "name": "Keywords",
      "scope": [
        "keyword.control",
        "keyword.operator.conditional",
        "keyword.query"
      ],
      "settings": { "foreground": "#b882ee", "fontStyle": "italic" }
    },
    {
      "name": "Keywords (Other)",
      "scope": ["keyword.other"],
      "settings": { "foreground": "#b882ee" }
    },

    {
      "name": "Namespace Keyword",
      "scope": ["keyword.other.namespace", "keyword.other.using"],
      "settings": { "foreground": "#6785f2" }
    },
    {
      "name": "Namespace",
      "scope": ["entity.name.type.namespace"],
      "settings": { "foreground": "#82bcff" }
    },

    {
      "name": "Numberics",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#D19A66" }
    },
    {
      "name": "Strings (Double Quotes)",
      "scope": [
        "string.quoted.double",
        "string.quoted.double punctuation",
        "meta.interpolation.cs"
      ],
      "settings": { "foreground": "#ce9178" }
    },
    {
      "name": "Interpolated Strings",
      "scope": ["punctuation.definition.interpolation"],
      "settings": { "foreground": "#e7a388" }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": { "foreground": "#e7a388", "fontStyle": "italic" }
    },
    {
      "name": "Chars (Single Quotes)",
      "scope": ["string.quoted.single", "string.quoted.single punctuation"],
      "settings": { "foreground": "#D19A66" }
    },

    {
      "name": "Variables",
      "scope": [
        "variable.other.readwrite",
        "variable.other.object",
        "variable.other.alias",
        "keyword.other.base",
        "keyword.other.this",
        "entity.name.variable"
      ],
      "settings": { "foreground": "#65b8c2" }
    },
    {
      "name": "Properties & Local Variables",
      "scope": [
        "variable.other.object.property",
        "support.variable",
        "entity.name.variable.local"
      ],
      "settings": { "foreground": "#D6D3DE" }
    },
    {
      "name": "Parameters",
      "scope": ["entity.name.variable.parameter"],
      "settings": { "foreground": "#D6D3DE", "fontStyle": "italic" }
    },

    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": { "foreground": "#b882ee" }
    },
    {
      "scope": ["constant.numeric.json"],
      "settings": { "foreground": "#82bcff" }
    },

    {
      "name": "Plain Markdown Text",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": { "foreground": "#8F8F99" }
    },
    {
      "name": "Italic Text",
      "scope": ["markup.italic", "markup.quote"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Bold Text",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": { "fontStyle": "bold" }
    },
    {
      "name": "Bold-Italic Text",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": { "fontStyle": "italic bold" }
    },

    {
      "name": "Heading Punctuation",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": { "foreground": "#447a81" }
    },
    {
      "name": "Heading Title",
      "scope": ["markup.heading.markdown"],
      "settings": { "foreground": "#65b8c2" }
    },

    {
      "name": "Blockquote",
      "scope": ["markup.quote punctuation.definition.quote"],
      "settings": { "foreground": "#6785E6" }
    },
    {
      "name": "Link",
      "scope": ["markup.underline.link.markdown"],
      "settings": { "foreground": "#ce9178" }
    },
    {
      "name": "Link Description",
      "scope": ["string.other.link.description.title.markdown"],
      "settings": { "foreground": "#b882ee" }
    },
    {
      "name": "Raw Block",
      "scope": ["markup.raw.block"],
      "settings": { "foreground": "#b882ee" }
    },
    {
      "name": "Raw String",
      "scope": ["markup.inline.raw", "punctuation.definition.raw.markdown"],
      "settings": { "foreground": "#DB605C" }
    },
    {
      "name": "Separator",
      "scope": ["meta.separator"],
      "settings": { "fontStyle": "bold", "foreground": "#65737E" }
    },

    {
      "name": "Lists",
      "scope": "markup.list",
      "settings": { "foreground": "#7fc8a0" }
    },
    {
      "name": "Sub-Lists (1)",
      "scope": ["markup.list markup.list"],
      "settings": { "foreground": "#6dad7f" }
    },
    {
      "name": "Sub-Lists (2)",
      "scope": ["markup.list markup.list markup.list"],
      "settings": { "foreground": "#5d9464" }
    },
    {
      "name": "Sub-Lists (3)",
      "scope": ["markup.list markup.list markup.list markup.list"],
      "settings": { "foreground": "#4f7a4d" }
    },
    {
      "name": "List Marker",
      "scope": ["markup.list punctuation", "markup.list beginning.punctuation"],
      "settings": { "foreground": "#588a6f" }
    },

    {
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#6785E6" }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#b882ee", "fontStyle": "italic" }
    },
    {
      "scope": ["text.xml"],
      "settings": { "foreground": "#D6D3DE" }
    },
    {
      "scope": ["punctuation.definition.tag.xml"],
      "settings": { "foreground": "#818188" }
    }
  ]
}
